// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products {
  id    Int    @id @default(autoincrement())
  name  String @unique
  price Int
  model String
}

model Users {
  id       Int    @id @default(autoincrement())
  name     String @unique
  email    String @unique
  password String
}
